  "devDependencies": {
    "codacy-coverage": "^1.1.3",
    "eslint": "^2.11.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "nsp": "^2.4.0",
    "semantic-release": "^4.3.5",
