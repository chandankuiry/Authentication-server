    "body-parser": "~1.15.2",
    "compression": "^1.6.2",
    "connect-flash": "~0.1.1",
    "connect-multiparty": "^2.1.0",
    "cookie-parser": "~1.4.3",
    "dms2dec": "^1.1.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "express-session": "~1.14.1",
    "imagemagick": "^0.1.3",
    "method-override": "~2.3.6",
    "mongoose": "~4.13.1",
    "morgan": "~1.7.0",
    "multer": "^1.1.0",
    "newrelic": "^1.28.0",
    "node-statsd": "^0.1.1",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "raven": "^0.11.0",
    "response-time": "^2.3.0"
